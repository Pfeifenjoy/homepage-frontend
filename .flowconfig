[ignore]
.*/build/.*
.*/node_modules/styled-components/.*

[include]

[libs]
flow-typed

[lints]

[options]
module.use_strict=true
suppress_type=$FlowIgnore
module.name_mapper.extension='html' -> '<PROJECT_ROOT>/flow/stub/file-loader.js'
